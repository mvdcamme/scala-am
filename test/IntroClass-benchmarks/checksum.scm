(define (scan-data)
  (random 1))
(define (sum-data char1 accumulator)
  (+ accumulator char1))
(define (conversion accumulator)
  (define i (/ accumulator 64))
  (- accumulator (* 64 i)))
(define (main)
  (display "Enter an abitrarily long string, ending with carriage return > ")
  (define (loop accumulator)
    (define input (scan-data))
    (define accumulator2 (sum-data input accumulator))
    (if (not (= input 99))
        (loop accumulator2)))
  (define remainder (conversion accumulator))
  (set! remainder (+ remainder 22))
  (set! input remainder)
  (display "Check sum is ") (display input) (newline))
  