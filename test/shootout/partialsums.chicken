(define (command-line-arguments) '(2500))
(define (argv) '())

(define (main args)
  (let ((n (if (null? args)
               1
               (car args)))

    (alt 1) (d2 0) (d3 0) (ds 0) (dc 0)
    (s0 0) (s1 0) (s2 0) (s3 0) (s4 0) (s5 0) (s6 0) (s7 0) (s8 0))

    (do ((d 1 (+ d 1)))
        ((< n d))

      (set! d2 (* d d))
      (set! d3 (* d2 d))
      (set! ds (sin d))
      (set! dc (cos d))

      (set! s0 (+ s0 (expt (/ 2 3) (- d 1))))
      (set! s1 (+ s1 (/ 1 (sqrt d))))
      (set! s2 (+ s2 (/ 1 (* d (+ d 1)))))
      (set! s3 (+ s3 (/ 1 (* d3 (* ds ds)))))
      (set! s4 (+ s4 (/ 1 (* d3 (* dc dc)))))
      (set! s5 (+ s5 (/ 1 d)))
      (set! s6 (+ s6 (/ 1 d2)))
      (set! s7 (+ s7 (/ alt d)))
      (set! s8 (+ s8 (/ alt (- (* 2 d) 1))))
      (set! alt (- alt))) 

    (display s0) (newline)
    (display s1) (newline)
    (display s2) (newline)
    (display s3) (newline)
    (display s4) (newline)
    (display s5) (newline)
    (display s6) (newline)
    (display s7) (newline)
    (display s8) (newline)))

(main (command-line-arguments))

