(define (command-line-arguments) '())
(define (argv) '())

(define (sum-harmonic4 n)
  (do ((i 1 (+ i 1))
       (sum 1.0 (+ sum (/ ifloat)))
       (ifloat 2.0 (+ ifloat 1)) )
      ((>= i n) sum) ) )

(sum-harmonic4 100000)
