;;;; -Ob

(define (command-line-arguments) '())

(define (ack m n)
  (cond ((zero? m) (+ n 1))
        ((zero? n) (ack (- m 1) 1))
        (else      (ack (- m 1) (ack m (- n 1))))))

(define (main args)
  (let ((n (if (null? args)
               1
               (string->number (car args)) ) ) )
    (display "Ack(3,") (display n) (display "): ")(display (ack 1 n))))

(main (command-line-arguments))
