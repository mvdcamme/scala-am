#!/bin/bash

trap "exit" INT

for((o=0;o<8;o++))
do
	for i in {1..1}
	do
		for testFile in ack.scm collatz.scm count.scm dderiv.scm fact.scm fib.scm gcipd.scm loop2.scm mut-rec.scm rotate.scm widen.scm closed.scm
		do
			echo "run -m Hybrid -c -l Concrete -f test/$testFile --tracing t --threshold 10 --optimized t --switch f -o $o -b \"benchmark results/tracing_$o.txt\" " | sbt
		done
	done
done
